all: output

output: time_tests.o vector_generator.o matrix.o
	g++ -g -Wall -O3 time_tests.o vector_generator.o matrix.o -o output

time_tests.o: time_tests.cpp
	g++ -g -Wall -O3 -c time_tests.cpp

vector_generator.o: vector_generator.hpp vector_generator.cpp
	g++ -g -Wall -O3 -c vector_generator.cpp

matrix.o: matrix.hpp matrix.cpp
	g++ -g -Wall -O3 -c matrix.cpp

clean:
	rm *.o output